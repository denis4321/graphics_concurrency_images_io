package threadex_.priority;

public class ConcurrentThread extends Thread{

    public ConcurrentThread(String name, int priority) {
        setName(name);
        setPriority(priority);
    }

    public void run(){
        for (int i=0; i<100; i++){
            System.out.println(getName()+" : "+i);
            for (long j=0; j<10000000; j++){}
        }
    }
}
