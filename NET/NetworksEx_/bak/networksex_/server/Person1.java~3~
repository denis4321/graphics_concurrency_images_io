public class Person1 {
   private String name;
   private int amount;

    public void setName(String name){
        this.name=name;
    }

    public void setOder() throws Exception {
        if (amount==0)throw new Exception("ADSFS");
        this.amount--;
    }

    public int getAmount(){
            return amount;
    }

    public void setAmount(int amount){
        this.amount=amount;
    }

    public String getName(){
        return name;
    }
}
