public class poshThread extends Thread {
    JLabel lab;
    Thread t;
    public poshThread(JLabel lab,Thread t) {
    this.lab=lab;
    this.t=t;
    }

    public void rr(JLabel lab){
    //lab.
    }

    public void run(){
        while (true){
            lab.setText(System.currentTimeMillis()+"-----------"+t.getState());
        }
    }
}
